<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='fotiusnet'>

  <inherits name="com.sencha.gxt.desktop.Desktop"/>

  <entry-point class='com.fotius.client.FotiusNet'/>

  <set-property name="gxt.user.agent" value="gecko1_8" />
  <set-property name="user.agent.os" value="windows" />

  <source path='client'/>
  <source path='shared'/>

  <!--superdevmode support-->
  <add-linker name="xsiframe"/>

  <set-configuration-property name="devModeRedirectEnabled" value="true"/>

  <set-property name="compiler.useSourceMaps" value="true" />

</module>
